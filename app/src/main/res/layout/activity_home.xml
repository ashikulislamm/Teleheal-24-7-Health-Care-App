<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home"
    tools:context=".Home">

    <RelativeLayout
        android:id="@+id/profile"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_x="300dp"
        android:layout_y="50dp"
        android:background="@drawable/custom_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/user" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginLeft="11dp"
            android:textStyle="bold"
            android:textSize="8dp"
            android:textColor="@color/white"
            android:text=" Profile" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/appointment"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_x="40dp"
        android:layout_y="180dp"
        android:background="@drawable/home_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/bappointment"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="45dp"
            app:srcCompat="@drawable/appointment" />

        <TextView
            android:id="@+id/bookappointmnet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:layout_marginLeft="15dp"
            android:textStyle="bold"
            android:textSize="17dp"
            android:text="       Book\n Appointment" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/labtest"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_x="230dp"
        android:layout_y="180dp"
        android:background="@drawable/home_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/test" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:layout_marginLeft="35dp"
            android:textStyle="bold"
            android:textSize="18dp"
            android:text="Lab Test" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/article"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_x="230dp"
        android:layout_y="350dp"
        android:background="@drawable/home_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/article" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:layout_marginLeft="40dp"
            android:textStyle="bold"
            android:textSize="18dp"
            android:text="Health \nArticle" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/medicine"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_x="40dp"
        android:layout_y="350dp"
        android:background="@drawable/home_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView

            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/medicine" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="72dp"
            android:layout_marginLeft="30dp"
            android:textStyle="bold"
            android:textSize="17dp"
            android:text="     Buy\n Medicine" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/order"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_x="40dp"
        android:layout_y="520dp"
        android:background="@drawable/home_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView

            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/order" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:layout_marginLeft="40dp"
            android:textStyle="bold"
            android:textSize="17dp"
            android:text="  Order\n Details" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/reminder"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_x="230dp"
        android:layout_y="520dp"
        android:background="@drawable/home_btn"
        android:elevation="20dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="40dp"
            app:srcCompat="@drawable/bell" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="75dp"
            android:layout_marginLeft="25dp"
            android:textStyle="bold"
            android:textSize="18dp"
            android:text=" Reminder" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="20dp"
        android:layout_y="50dp"
        android:textSize="40dp"
        android:textStyle="bold"
        android:textColor="@color/Lavender"
        android:text="TELEHEAL" />

    <Button
        android:id="@+id/logout"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_x="37dp"
        android:layout_y="720dp"
        android:paddingLeft="-30dp"
        android:background="@drawable/custom_btn2"
        android:text="Logout" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_x="120dp"
        android:layout_y="730dp"
        app:srcCompat="@drawable/logout" />

</AbsoluteLayout>